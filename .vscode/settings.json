{
  "code-runner.executorMapByFileExtension": {
    ".kt": "cd $dir && python -u mykotlinc.py $fileName --clear --params \"-w --no-rebuild --configuration-cache --parallel --daemon --build-cache\""
  },
  "code-runner.executorMap": {
    //调用MSVC
    //调用 cl.exe 时禁用版本信息的显示，/nologo 参数。这个参数可以防止 cl.exe 在编译过程中显示版本信息和版权信息。
    //W4 最严格检查
    ///WX 将警告视为错误
    // /O1 最大优化(优选空间)          /O2 最大优化(优选速度)
    // /Ob<n> 内联扩展(默认 n=0)         /Od 禁用优化(默认)
    // /Og 启用全局优化                  /Oi[-] 启用内部函数
    // /Os 优选代码空间                  /Ot 优选代码速度
    // /Ox 优化(优选速度)                /Oy[-] 启用帧指针省略
    // /Gm[-] 启用最小重新生成
    //最小重新生成 /Gm
    ///std:<c++14|c++17|c++20|c++latest>
    //c++最新 /std:c++latest
    ///std:<c11|c17|clatest> C 标准版本
    //c最新 /std:clatest
    ///Fe: <file> 命名可执行文件
    ///Fo: <file> 命名对象文件
    //   "c": "cd $dir && cl /std:clatest /W4 /O2 /nologo /source-charset:utf-8 $fileName /EHsc /Fe\"$workspaceRoot\\$fileNameWithoutExt\" /Fo\"$workspaceRoot\\$fileNameWithoutExt\" && $workspaceRoot\\$fileNameWithoutExt",
    //   "cpp": "cd $dir && cl /std:c++latest /W4 /O2 /nologo /source-charset:utf-8 $fileName /EHsc /Fe\"$workspaceRoot\\$fileNameWithoutExt\" /Fo\"$workspaceRoot\\$fileNameWithoutExt\" && $workspaceRoot\\$fileNameWithoutExt"
    // },
    "c": "cd $dir && cl /std:clatest /W4 /O2 /nologo /source-charset:utf-8 $fileName /EHsc /Fe\"$fileNameWithoutExt\" /Fo\"$fileNameWithoutExt\" && $fileNameWithoutExt",
    "cpp": "cd $dir && cl /std:c++latest /W4 /O2 /nologo /source-charset:utf-8 $fileName /EHsc /Fe\"$fileNameWithoutExt\" /Fo\"$fileNameWithoutExt\" && $fileNameWithoutExt"
  },
